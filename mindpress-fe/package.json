{
  "private": true,
  "name": "mindpress",
  "version": "1.0.0",
  "description": "Press your mind(markdown) gracefully, which is powered by [nuxt.js 3.0](https://v3.nuxtjs.org) and [nuxt content 2.0](https://github.com/nuxt/content).",
  "keywords": [
    "nuxt",
    "content",
    "mindpress"
  ],
  "repository": "aborn/mindpress",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port=7001",
    "build:ssg": "MINDPRESS_MODE=SSG nuxt build",
    "build:fcm": "cross-env MINDPRESS_MODE=FCM nuxt build",
    "build:scm": "cross-env MINDPRESS_MODE=SCM nuxt build",
    "deploy:fcm": "PORT=7010 node .output/server/index.mjs",
    "deploy:scm": "PORT=3010 node .output/server/index.mjs",
    "dev:ssg": "cross-env MINDPRESS_MODE=SSG nuxt dev --port=7001",
    "dev:scm": "cross-env MINDPRESS_MODE=SCM nuxt dev --port=7001",
    "dev:fcm": "cross-env MINDPRESS_MODE=FCM nuxt dev --port=7001",
    "generate": "BUILD_MODE=ghpages MINDPRESS_MODE=SSG nuxt generate",
    "preview": "nuxt preview",
    "start": "nuxt build && PORT=8001 node .output/server/index.mjs",
    "deploy": "touch ./dist/.nojekyll && push-dir --dir=dist --branch=gh-pages --cleanup",
    "gh": "npx nuxi generate && npx serve -s ./dist",
    "docker:build:fcm": "BUILD_MODE=docker MINDPRESS_MODE=FCM nuxt build",
    "docker:deploy:fcm": "PORT=7010 node .output/server/index.mjs",
    "docker:build:scm": "BUILD_MODE=docker MINDPRESS_MODE=SCM nuxt build",
    "docker:deploy:scm": "PORT=3010 node .output/server/index.mjs",
    "test": "vitest"
  },
  "devDependencies": {
    "@nuxt/content": "2.12.1",
    "@nuxt/schema": "3.12.2",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/color-mode": "^3.4.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^24.1.0",
    "nuxt": "3.12.2",
    "prettier": "^3.3.2",
    "push-dir": "^0.4.1",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.2.4",
    "@codemirror/language-data": "^6.4.1",
    "@codemirror/state": "^6.4.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@nuxt/kit": "^3.12.2",
    "@nuxt/ui": "^2.17.0",
    "@tailwindcss/typography": "^0.5.13",
    "axios": "^1.1.3",
    "codemirror": "^6.0.1",
    "debug": "^4.3.5",
    "hast-util-from-html": "^2.0.1",
    "hast-util-to-mdast": "^10.1.0",
    "highlight.js": "^11.10.0",
    "katex": "^0.16.11",
    "marked": "^13.0.2",
    "marked-katex-extension": "^5.0.2",
    "md-editor-v3": "^4.15.6",
    "minisearch": "^6.3.0",
    "parse5": "^7.1.2",
    "rehype-raw": "^7.0.0",
    "remark-emoji": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdc": "^3.2.1",
    "remark-rehype": "^11.1.0",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.4",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "uuid": "^10.0.0"
  }
}
